print("\n Data Diri Mahasiswa")

nama=input("Masukkan Nama : ")
nim=input("Masukkan Nim Anda : ")
prodi=input("Masukkan Prodi : ")
alamat=input("Masukkan Alamat : ")
usia=int(input("Masukkan Usia : "))
tanggal_lahir=int(input("Masukkan Tanggal Lahir : "))
bulan_lahir=int(input("Masukkan Bulan Lahir : "))
tahun_lahir=int(input("Masukkan Tahun Lahir : "))
sd=input("Maaukkan Sekolah Sd : ")
smp=input("Maaukkan Sekolah smp : ")
sma=input("Masukkan Sekolah Sma : ")
perguruan_tinggi=input("Masukkan Perguruan Tinggi : ")

print("=="*23)
print("Nama :",nama)
print("Nim :",nim)
print("Prodi :",prodi)
print("Alamat :",alamat)
print("Usia :",usia)
print("Tanggal Lahir :",tanggal_lahir)
print("Bulan Lahir :",bulan_lahir)
print("Tahun Lahir :",tahun_lahir)
print("=="*23)
print("Sd :",sd)
print("Smp :",smp)
print("Sma :",sma)
print("Perguruan Tinggi :",perguruan_tinggi)
print("=="*23)

print("\n Terima Kasih")

